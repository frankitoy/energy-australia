{
  "name": "energy-australia",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all lint:fix serve",
    "build": "ng build --build-optimizer=true --prod --source-map=false --base-href=/",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "e2e": "ng e2e -wu false",
    "e2e:ci": "/usr/bin/xvfb-run --server-args=\"-screen 0 1920x1920x24\" npm run e2e",
    "lint": "ng lint",
    "lint:fix": "ng lint energy-australia --fix",
    "precommit": "npm-run-all lint:fix",
    "prepush": "npm-run-all lint test",
    "pre-release": "npm-run-all lint test build",
    "serve": "ng serve --proxy-config proxy.config.json --source-map=true --base-href=/ --port 4100",
    "test": "ng test --watch=false --code-coverage"
  },
  "private": true,
  "engines": {
    "node": ">=8.11.4",
    "npm": ">=5.6.0"
  },
  "dependencies": {
    "@angular-redux/store": "^9.0.0",
    "@angular/animations": "~7.0.0",
    "@angular/cdk": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/material": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "angular-in-memory-web-api": "0.7.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "lodash": "4.17.11",
    "node-sass": "4.11.0",
    "redux": "4.0.1",
    "rxjs": "~6.3.3",
    "rxjs-compat": "6.1.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.6",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "4.14.118",
    "chalk": "2.3.2",
    "chokidar": "2.0.3",
    "codelyzer": "~4.5.0",
    "gulp": "4.0.0",
    "husky": "0.14.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "4.0.0",
    "karma-cli": "1.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-istanbul-threshold": "~1.2.2",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "1.2.0",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-spec-reporter": "0.0.32",
    "node-exec-promise": "1.0.2",
    "npm-run-all": "4.1.2",
    "protractor": "~5.4.0",
    "rxjs-tslint": "0.1.5",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "webpack-bundle-analyzer": "3.0.3",
    "yargs": "11.0.0"
  }
}
