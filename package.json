{
  "name": "energy-australia",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all lint:fix serve",
    "build": "ng build --build-optimizer=true --prod --source-map=false --base-href=/",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "e2e": "ng e2e --port 4100",
    "e2e:ci": "/usr/bin/xvfb-run --server-args=\"-screen 0 1920x1920x24\" npm run e2e",
    "lint": "ng lint",
    "lint:fix": "ng lint energy-australia --fix",
    "precommit": "npm-run-all lint:fix",
    "prepush": "npm-run-all lint test",
    "pre-release": "npm-run-all lint test build",
    "serve": "ng serve --proxy-config proxy.config.json",
    "test": "ng test --watch=false --code-coverage"
  },
  "private": true,
  "engines": {
    "node": ">=8.11.4",
    "npm": ">=5.6.0"
  },
  "dependencies": {
    "@angular-redux/store": "~10.0.0",
    "@angular/animations": "~10.0.11",
    "@angular/cdk": "~10.1.3",
    "@angular/common": "~10.0.11",
    "@angular/compiler": "~10.0.11",
    "@angular/core": "~10.0.11",
    "@angular/forms": "~10.0.11",
    "@angular/http": "~7.2.16",
    "@angular/material": "~10.1.3",
    "@angular/platform-browser": "~10.0.11",
    "@angular/platform-browser-dynamic": "~10.0.11",
    "@angular/router": "~10.0.11",
    "angular-in-memory-web-api": "~0.11.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "lodash": "4.17.21",
    "node-sass": "4.14.1",
    "redux": "~4.0.5",
    "rxjs": "~6.6.2",
    "rxjs-compat": "~6.6.2",
    "zone.js": "~0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1000.7",
    "@angular/cli": "10.0.7",
    "@angular/compiler-cli": "10.0.11",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "~4.14.159",
    "@types/node": "~14.6.0",
    "chalk": "~4.1.0",
    "chokidar": "3.4.2",
    "codelyzer": "~4.5.0",
    "cypress": "5.0.0",
    "gulp": "~4.0.2",
    "husky": "~4.2.5",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-istanbul-threshold": "1.2.3",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "1.2.0",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-spec-reporter": "0.0.32",
    "node-exec-promise": "1.0.2",
    "npm-run-all": "4.1.2",
    "protractor": "7.0.0",
    "rxjs-tslint": "0.1.8",
    "ts-node": "~8.10.2",
    "tslint": "~5.11.0",
    "typescript": "~4.0.2",
    "webpack-bundle-analyzer": "3.8.0",
    "yargs": "15.4.1"
